{
    "family": "mesh-hello-service",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "mesh-hello-service",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8090,
                    "hostPort": 8090
                }
            ]
        }
    ]
}
